<% if @year_in_review_release.persisted? %>
  $('#year_in_review_music_releases').append('<%= escape_javascript render(partial: 'library/music/year_in_review_releases/year_in_review_release', locals: { year_in_review_release: @year_in_review_release }) %>');
  $('#bootstrap_modal').modal('hide')
<% else %>
  $('#bootstrap_modal').html('<%= escape_javascript render(partial: 'library/music/year_in_review_releases/form') %>');
<% end %>

<% if @notice.present? %>
alert('<%= @notice %>');
<% end %>