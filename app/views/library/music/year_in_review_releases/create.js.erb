<% if @year_in_review_release.persisted? %>
  $('#years_in_review').html('<%= escape_javascript render(partial: 'library/music/year_in_review_releases/collection') %>');
  window.VoluntaryMusicMetadataEnrichment.Library.YearInReviewReleases.IndexView.makeCollectionSortable();
  $('#bootstrap_modal').modal('hide')
<% else %>
  $('#bootstrap_modal').html('<%= escape_javascript render(partial: 'library/music/year_in_review_releases/form') %>');
<% end %>

<% if @notice.present? %>
alert('<%= @notice %>');
<% end %>