<h3><%= t('music_metadata_enrichment_groups.new.title') %></h3>

<%= simple_form_for(@group, url: music_groups_path, method: :post, html: {class: 'form-horizontal'}) do |f| %>
  <%= render partial: 'shared/form/error_messages', locals: { resource: @group } %>
  <%= f.input :name %>
  <%= f.submit %>
<% end %>